<p id="notice"><%= notice %></p>

<h1>Prints</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>In stock</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prints.each do |print| %>
      <tr>
        <td><%= print.title %></td>
        <td><%= print.description %></td>
        <td><%= print.price %></td>
        <td><%= print.in_stock %></td>
        <td><%= print.image_url %></td>
        <td><%= link_to 'Show', print %></td>
        <td><%= link_to 'Edit', edit_print_path(print) %></td>
        <td><%= link_to 'Destroy', print, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Print', new_print_path %>
